<h3 class="page-header">
  User Profile
</h3>

<p><%= @user.email %></p>

<p><%= link_to "Update Password", edit_user_path(@user) %> <%= link_to "Delete Account", user_path(@user), method: :delete, data: { confirm: 'Are you sure?' } %></p>

<p>Rooms</p>
    <% @user.rooms.uniq.each do |affiliation| %>
    	<li><%= affiliation.title %></li>
    <% end %>